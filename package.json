{
  "name": "subscription-central",
  "version": "0.0.1",
  "private": false,
  "scripts": {
    "analyze": "npm run build:prod && source-map-explorer 'dist/js/*.js'",
    "build": "webpack --config config/prod.webpack.config.js",
    "build:prod": "NODE_ENV=production webpack --config config/prod.webpack.config.js",
    "deploy": "npm-run-all build:prod lint test",
    "format": "prettier \"src/**/*.{js,tsx,html,scss}\" --write",
    "lint": "npm-run-all lint:*",
    "lint:js": "eslint config src",
    "lint:js:fix": "eslint config src --fix",
    "lint:sass": "stylelint 'src/**/*.scss' --config .stylelintrc.json",
    "lint:staged": "pretty-quick --staged && lint-staged",
    "nightly": "npm run deploy",
    "prod": "NODE_ENV=production webpack-dev-server --config config/dev.webpack.config.js",
    "proxy": "SPANDX_CONFIG=profiles/local-frontend.js sh ../insights-proxy/scripts/run.sh",
    "server:ctr": "node src/server/generateServerKey.js",
    "start": "NODE_ENV=development webpack serve --config config/dev.webpack.config.js",
    "start:proxy": "run-p proxy start",
    "test": "jest --verbose",
    "test:watch": "jest --watch",
    "verify": "npm-run-all build lint test"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint:staged"
    }
  },
  "lint-staged": {
    "*.{js,scss,ts,tsx,html}": "eslint --cache --fix"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/stories/*"
    ],
    "coverageDirectory": "./coverage/",
    "moduleNameMapper": {
      "\\.(css|scss)$": "identity-obj-proxy"
    },
    "roots": [
      "<rootDir>/src/"
    ],
    "setupFiles": [
      "<rootDir>/config/setupTests.js"
    ]
  },
  "transformIgnorePatterns": [
    "/node_modules/(?!@redhat-cloud-services)",
    "/node_modules/(?!@patternfly)"
  ],
  "dependencies": {
    "@babel/runtime": "7.10.0",
    "@patternfly/patternfly": "^4.42.2",
    "@patternfly/react-core": "^4.90.2",
    "@patternfly/react-table": "^4.20.15",
    "@redhat-cloud-services/frontend-components": "^3.1.4",
    "@redhat-cloud-services/frontend-components-notifications": "^3.1.0",
    "@redhat-cloud-services/frontend-components-utilities": "^3.0.2",
    "babel-plugin-transform-imports": "^2.0.0",
    "classnames": "2.2.6",
    "jest-fetch-mock": "^3.0.3",
    "js-cookie": "^2.2.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-query": "^3.5.1",
    "react-redux": "7.2.2",
    "react-router-dom": "^5.2.0",
    "redux": "4.0.5",
    "redux-logger": "3.0.6",
    "redux-promise-middleware": "6.1.2",
    "ts-loader": "^8.0.7",
    "typescript": "^4.0.5"
  },
  "devDependencies": {
    "@babel/core": "7.12.13",
    "@babel/plugin-proposal-object-rest-spread": "7.12.13",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-transform-runtime": "7.12.13",
    "@babel/preset-env": "7.12.13",
    "@babel/preset-flow": "7.9.0",
    "@babel/preset-react": "7.12.13",
    "@babel/preset-typescript": "^7.12.7",
    "@redhat-cloud-services/eslint-config-redhat-cloud-services": "^1.0.0",
    "@redhat-cloud-services/frontend-components-config": "^4.0.12",
    "@testing-library/react": "^11.2.3",
    "@types/jest": "^26.0.20",
    "@types/js-cookie": "^2.2.6",
    "@types/lodash": "^4.14.165",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-query": "^1.1.2",
    "@types/react-redux": "^7.1.16",
    "@types/react-router": "^5.1.8",
    "@types/react-router-dom": "^5.1.6",
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "@typescript-eslint/parser": "^4.9.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^26.6.3",
    "babel-plugin-dual-import": "1.2.1",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-transform-imports": "^2.0.0",
    "cross-fetch": "3.0.4",
    "css-loader": "5.0.1",
    "eslint": "7.19.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-loader": "4.0.2",
    "eslint-plugin-react": "7.20.0",
    "eslint-plugin-react-hooks": "4.0.4",
    "husky": "^4.3.5",
    "identity-obj-proxy": "3.0.0",
    "jest": "26.6.3",
    "lint-staged": "^10.5.3",
    "node-sass": "4.14.1",
    "npm-run-all": "4.1.5",
    "null-loader": "^4.0.1",
    "postcss": "7.0.31",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "prop-types": "15.7.2",
    "source-map-explorer": "^2.5.1",
    "stylelint": "13.9.0",
    "stylelint-config-recommended-scss": "4.2.0",
    "stylelint-scss": "3.18.0",
    "webpack": "5.24.4",
    "webpack-bundle-analyzer": "4.4.0",
    "webpack-cli": "4.5.0"
  },
  "insights": {
    "appname": "subscription-central"
  }
}
