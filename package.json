{
  "name": "subscription-central",
  "version": "0.0.1",
  "private": false,
  "scripts": {
    "analyze": "npm run build:prod && source-map-explorer 'dist/js/*.js'",
    "build": "webpack --config config/prod.webpack.config.js",
    "build:prod": "NODE_ENV=production webpack --config config/prod.webpack.config.js",
    "deploy": "npm-run-all build:prod lint test",
    "format": "prettier \"src/**/*.{js,tsx,html,scss}\" --write",
    "lint": "npm-run-all lint:*",
    "lint:js": "eslint config src",
    "lint:js:fix": "eslint config src --fix",
    "lint:sass": "stylelint 'src/**/*.scss' --config .stylelintrc.json",
    "lint:staged": "pretty-quick --staged && lint-staged",
    "nightly": "npm run deploy",
    "postisntall": "rm -rf .cache",
    "prepare": "node ./prepare.js",
    "prod": "NODE_ENV=production webpack-dev-server --config config/dev.webpack.config.js",
    "server:ctr": "node src/server/generateServerKey.js",
    "start": "NODE_ENV=development webpack serve --config config/dev.webpack.config.js",
    "start:beta": "BETA=true NODE_ENV=development webpack serve --config config/dev.webpack.config.js",
    "test": "TZ=UTC jest --verbose --no-cache",
    "test:watch": "jest --watch",
    "verify": "npm-run-all build lint test"
  },
  "overrides": {
    "glob-parent": "^6.0.1",
    "hosted-git-info": "^3.0.8",
    "url-parse": "^1.5.3",
    "axios": "^0.21.4",
    "moment": "2.29.4"
  },
  "lint-staged": {
    "*.{js,scss,ts,tsx,html}": "eslint --cache --fix"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/stories/*",
      "!src/App.js",
      "!src/AppEntry.js",
      "!src/bootstrap-dev.js",
      "!src/bootstrap.js",
      "!src/entry-dev.js",
      "!src/entry.js",
      "!src/Routes.tsx",
      "!src/store/index.js"
    ],
    "coverageDirectory": "./coverage/",
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 85,
        "statements": 80
      }
    },
    "moduleNameMapper": {
      "\\.(css|scss)$": "identity-obj-proxy"
    },
    "clearMocks": true,
    "roots": [
      "<rootDir>/src/"
    ],
    "setupFiles": [
      "<rootDir>/config/setupTests.js"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.js"
    ],
    "testEnvironment": "jsdom",
    "testRunner": "jest-jasmine2"
  },
  "transformIgnorePatterns": [
    "/node_modules/(?!@patternfly)"
  ],
  "dependencies": {
    "@babel/runtime": "^7.22.6",
    "@patternfly/patternfly": "^5.0.0",
    "@patternfly/react-core": "^5.0.0",
    "@patternfly/react-table": "^5.0.0",
    "@redhat-cloud-services/frontend-components": "^4.0.0",
    "@redhat-cloud-services/frontend-components-utilities": "^4.0.0",
    "babel-plugin-transform-imports": "^2.0.0",
    "classnames": "2.3.2",
    "jest-fetch-mock": "^3.0.3",
    "js-cookie": "^3.0.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.36.0",
    "react-query": "^3.39.3",
    "react-redux": "^8.1.1",
    "react-router-dom": "^6.7.0",
    "redux": "^4.2.1",
    "redux-logger": "3.0.6",
    "redux-promise-middleware": "6.1.3",
    "semver": "^7.3.8",
    "ts-loader": "^9.4.4",
    "typescript": "^4.9.5",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.22.9",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-transform-runtime": "^7.22.9",
    "@babel/preset-env": "^7.22.9",
    "@babel/preset-flow": "7.22.5",
    "@babel/preset-react": "7.22.5",
    "@babel/preset-typescript": "^7.18.6",
    "@faker-js/faker": "^8.0.2",
    "@redhat-cloud-services/eslint-config-redhat-cloud-services": "^1.2.4",
    "@redhat-cloud-services/frontend-components-config": "^4.7.2",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.1.5",
    "@types/jest": "^29.5.3",
    "@types/js-cookie": "^3.0.3",
    "@types/lodash": "^4.14.195",
    "@types/react": "^17.0.44",
    "@types/react-dom": "^17.0.11",
    "@types/react-redux": "^7.1.25",
    "@types/react-router": "^5.1.20",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "babel-jest": "^29.4.3",
    "babel-plugin-dual-import": "1.2.1",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-transform-imports": "^2.0.0",
    "bdd-lazy-var": "^2.6.1",
    "cross-fetch": "^4.0.0",
    "css-loader": "^6.8.1",
    "escape-string-regexp": "^5.0.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "fishery": "^2.2.2",
    "husky": "^8.0.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.6.1",
    "jest-environment-jsdom": "^29.6.1",
    "jest-jasmine2": "^29.6.1",
    "lint-staged": "^13.1.2",
    "load-json-file": "^7.0.1",
    "node-sass": "^9.0.0",
    "npm-run-all": "4.1.5",
    "null-loader": "^4.0.1",
    "postcss": "^8.4.26",
    "prettier": "^2.8.8",
    "pretty-quick": "^3.1.3",
    "prop-types": "^15.8.1",
    "sass-loader": "^13.3.2",
    "source-map-explorer": "^2.5.3",
    "stylelint": "^15.2.0",
    "stylelint-config-recommended-scss": "^12.0.0",
    "stylelint-scss": "^5.0.1",
    "webpack-bundle-analyzer": "^4.9.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "insights": {
    "appname": "manifests"
  }
}
